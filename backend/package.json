{
  "name": "uganda-gynae-backend",
  "version": "1.0.0",
  "description": "Backend API for Uganda Gynecology Platform",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "build": "npm run build:clean && npm run build:compile",
    "build:clean": "rimraf dist",
    "build:compile": "babel src -d dist",
    "test": "jest",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix"
  },
  "keywords": [
    "supabase",
    "express",
    "api",
    "gynecology",
    "healthcare"
  ],
  "author": "Uganda Gynecology Platform Team",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.56.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "@babel/cli": "^7.23.6",
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
